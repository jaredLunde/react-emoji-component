{
  "name": "react-emoji-component",
  "version": "4.0.0-beta.0",
  "description": "A component for converting unicode emojis to the open EmojiOne emoji set.",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "esnext": "es/index.js",
  "bin": {
    "copy-emoji-assets": "assets/copyBin.js"
  },
  "repository": "https://github.com/jaredLunde/react-emoji-component",
  "author": "Jared Lunde",
  "license": "MIT",
  "sideEffects": false,
  "private": false,
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@inst-app/babel-preset-esx": "^1.0.2",
    "@inst-app/babel-preset-react": "^1.0.0",
    "cross-env": "^5.2.0",
    "css": "^2.2.4",
    "emojione-assets": "^4.0.0",
    "fs-extra": "^7.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "empty": "^0.10.1",
    "grapheme-splitter": "^1.0.3"
  },
  "peerDependencies": {
    "prop-types": "^15.x",
    "react": "^16.x"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:cjs && npm run prettier:es && npm run prettier:cjs",
    "build:es": "rimraf es && cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "rimraf cjs && cross-env NODE_ENV=production BABEL_ENV=cjs babel src --out-dir cjs",
    "create-emoji-data": "node scripts/createEmojiData",
    "prettier:es": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma es5 --write \"es/**/*.js\"",
    "prettier:cjs": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma es5 --write \"cjs/**/*.js\"",
    "watch:es": "yarn build:es --watch",
    "watch:cjs": "yarn build:cjs --watch"
  }
}
